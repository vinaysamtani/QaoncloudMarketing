<script type="text/javascript">
$(document).ready(function() {
	// navigation click actions	
	var drop = 0;
	var item;
	$('.scroll-link').on('click', function(event){
		event.preventDefault();
		var sectionID = $(this).attr("data-id");
		if(drop==0)
		{
			item = sectionID + "-item";
			$('#' + item).addClass("active-shining");
			$('#'+ item > '.scroll-link').css({'color':'#6CCFF4'});
			scrollToID('#' + sectionID, 750);
			drop=1;
		}
		else
		{
			$('#' + item).removeClass("active-shining");
			item = sectionID + "-item";
			$('#' + item).addClass("active-shining");
			scrollToID('#' + sectionID, 750);
		}	
	});
	// scroll to top action
	$('.scroll-top').on('click', function(event) {
		event.preventDefault();
		$('#' + item).removeClass("active-shining");
		$('html, body').animate({scrollTop:0}, 'slow'); 		
	});
	// mobile nav toggle
	$('#nav-toggle').on('click', function (event) {
		event.preventDefault();
		$('#main-nav').toggleClass("open");
	});

	$(".my-nav").hover(function(){
		$("#services-item").hover(function(){
		$('#services-item').addClass("shining");
		},function(){
		$('#services-item').removeClass("shining");
		});
		$("#process-item").hover(function(){
		$('#process-item').addClass("shining");
		},function(){
		$('#process-item').removeClass("shining");
		});
		$("#blog-item").hover(function(){
		$('#blog-item').addClass("shining");
		},function(){
		$('#blog-item').removeClass("shining");
		});
		$("#success-stories-item").hover(function(){
		$('#success-stories-item').addClass("shining");
		},function(){
		$('#success-stories-item').removeClass("shining");
		});
		$("#pricing-item").hover(function(){
		$('#pricing-item').addClass("shining");
		},function(){
		$('#pricing-item').removeClass("shining");
		});
		$("#about-us-item").hover(function(){
		$('#about-us-item').addClass("shining");
		},function(){
		$('#about-us-item').removeClass("shining");
		});
		$("#register-item").hover(function(){
		$('#register-item').addClass("shining");
		},function(){
		$('#register-item').removeClass("shining");
		});
	});
	
	});
// scroll function
function scrollToID(id, speed){
	var offSet = 50;
	var targetOffset = $(id).offset().top - offSet;
	var mainNav = $('#main-nav');
	$('html,body').animate({scrollTop:targetOffset}, speed);
	if (mainNav.hasClass("open")) {
		mainNav.css("height", "1px").removeClass("in").addClass("collapse");
		mainNav.removeClass("open");
	}
}
if (typeof console === "undefined") {
    console = {
        log: function() { }
    };
}
</script>
